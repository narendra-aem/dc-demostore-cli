{
  "name": "@amplience/dc-demostore-cli",
  "version": "2.0.1",
  "description": "Amplience Demo Store CLI",
  "man": "./dist/demo-cli.1",
  "bin": {
    "demostore": "./dist/index.js",
    "dc-demostore-cli": "./dist/index.js"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist && tsc && cross-os osbuild",
    "format:check": "prettier --check \"{app,src,test}/**/*.{ts,js}\"",
    "format": "prettier --write \"{app,src,test}/**/*.{ts,js}\"",
    "osbuild": {
      "darwin": "chmod +x dist/index.js",
      "linux": "chmod +x dist/index.js",
      "win32": "cd ."
    },
    "version:patch": "standard-version --release-as patch",
    "version:minor": "standard-version --release-as minor",
    "version:major": "standard-version --release-as major",
    "prepare-patch-release": "run-s build version:patch",
    "prepare-minor-release": "run-s build version:minor",
    "prepare-major-release": "run-s build version:major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amplience/dc-demostore-cli.git"
  },
  "files": [
    "dist/*"
  ],
  "keywords": [
    "cli",
    "automation",
    "amplience",
    "headless",
    "cms",
    "demo",
    "demostore"
  ],
  "author": "Amplience Innovation Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/amplience/dc-demostore-cli/issues"
  },
  "homepage": "https://github.com/amplience/dc-demostore-cli#readme",
  "dependencies": {
    "@rauschma/stringio": "^1.4.0",
    "adm-zip": "^0.5.12",
    "ajv": "^8.12.0",
    "algoliasearch": "^4.23.3",
    "async": "^3.2.5",
    "axios": "^0.27.2",
    "chalk": "^4.1.2",
    "cli-table": "^0.3.11",
    "dc-management-sdk-js": "^2.0.0",
    "enquirer": "^2.4.1",
    "fetch-retry": "^6.0.0",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "http-status-codes": "^2.3.0",
    "isomorphic-unfetch": "^4.0.2",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.7",
    "nanoid-dictionary": "^4.3.0",
    "p-throttle": "^4.1.1",
    "winston": "^3.13.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/async": "^3.2.24",
    "@types/cli-table": "^0.3.4",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.0",
    "@types/nanoid-dictionary": "^4.2.3",
    "@types/node": "^20.12.7",
    "@types/yargs": "^17.0.32",
    "cross-os": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "standard-version": "^9.3.2",
    "typescript": "^5.4.5"
  }
}
